	*fading: 45


	! black
  		*.color0:      	#191919
   		*.color8:      	#6C6E63
	! red
		*.color1:      	#B92A38
		*.color9:      	#D97A67
	! green
		*.color2:      	#569F66
		*.color10:     	#98C552
	! yellow
		*.color3:      	#D1BE7C
		*.color11:     	#FFEE87
	! blue
		*.color4:      	#4885BE
