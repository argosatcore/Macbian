	! magenta
		*.color5:     	#7164AD
		*.color13:     	#7C77AB
	! cyan
		*.color6:    	#309798
		*.color14:    	#78ADAF
	! white
		*.color7:     	#A2A089

		*.color15:    	#DEE0D5

!Transparency
	URxvt*transparent:      true 
	URxvt*depth:		32
	URxvt.background: 	[75]#101010


!Scrollbar: Disabled
	URxvt*scrollBar:      	false
	URxvt*scrollBar_right: 	false

!Fonts
!URxvt.font:xft:SauceCodePro Nerd Font Mono:size=10
	URxvt.font:xft:Hack:size=10.5
!	URxvt.font:xft:Iconoslata:size=10
!	URxvt.font:xft:Fira Code Retina:size=12
!	URxvt.font:xft:Monospace:size=12


	URxvt.letterSpace:	-1 
	URxvt.lineSpace: 	0

	*antialias:  true
	*autohint:   true
	Xft.antialias:		1
	Xft.hinting:		1
	Xft.autohint:		0
	Xft.hintstyle:		hintslight
	Xft.rgba:		rgb
	Xft.lcdfilter:		lcddefault

!Scroll back position
	! do not scroll with output
	URxvt*scrollTtyOutput: false

	! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
	URxvt*scrollWithBuffer: true

	! scroll back to the bottom on keypress
	URxvt*scrollTtyKeypress: true
:
	! Scroll like rxvt
	URxvt*scrollstyle: rxvt

! Common Keybinds for Navigations:
	URxvt.keysym.Shift-Up: 	command:\033]720;1\007
	URxvt.keysym.Shift-Down: command:\033]721;1\007
	URxvt.keysym.Control-Up: \033[1;5A
	URxvt.keysym.Control-Down: \033[1;5B
	URxvt.keysym.Control-Right: \033[1;5C
	URxvt.keysym.Control-Left: \033[1;5D
	URxvt.keysym.Shift-Control-V: perl:clipboard:paste
	URxvt.keysym.M-x: perl:clipboard:paste_xsel

! Font resizing:	
	URxvt.keysym.C-minus: font-size:decrease
	URxvt.keysym.C-plus:  font-size:increase
	URxvt.keysym.C-equal: font-size:show
	URxvt.font-size.step: 2


!Perl Extensions:

!	Clickables urls
		URxvt.perl-ext-common:  default,matcher,clipboard
!		URxvt.url-launcher:	 /usr/bin/xdg-open 
	
		URxvt.matcher.button:	 1
    
    		URxvt.underlineURLs: 	true
		URxvt.perl-ext:     	 default,url-select
		URxvt.keysym.M-u: 	   perl:url-select:select_next
		URxvt.urlLauncher:  	 firefox
		URxvt.underlineURLs: 	true

!	Bidirectional support
		URxvt.perl-ext: 	[other extensions],bidi
		URxvt.bidi.enabled: 	1
!	
!	Clipboard
		URxvt.keysym.Shift-Control-C:       perl:clipboard:copy
		URxvt.keysym.Shift-Control-V:       perl:clipboard:paste
		



